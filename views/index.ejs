<html>

<head>
  <title>Homepage</title>
  <!-- <link rel="stylesheet" href="/styles.css"> -->
</head>

<body>
  <ul>
    <% messages.forEach((message)=> { %>
      <li>

        <%= message.user %>
          <br>
          <%= message.text %>
            <br>
            <%= message.added %>
              <br>
              <a href="/details/<%= message.index %>">Open</a>
              <br>
              <br>
      </li>
      <% }); %>
  </ul>
  <a href="/new">New Message Page</a>

</body>

</html>